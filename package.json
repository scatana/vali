{
  "name": "vali",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:fr": "ng serve --configuration development,fr",
    "start:ro": "ng serve --configuration development,ro",
    "extract-i18n": "ng run vali:extract-i18n-merge",
    "test": "ng test",
    "test:ci": "ng test --configuration ci",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "dev:ssr": "ng run vali:serve-ssr",
    "build:ssr": "ng build && ng run vali:server",
    "serve:ssr": "node dist/vali/server/main.js",
    "prerender": "ng run vali:prerender",
    "dist": "run-s test:ci prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/platform-server": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@nguniversal/express-engine": "~13.1.0",
    "bootstrap": "~5.1.0",
    "express": "~4.17.0",
    "rxjs": "~7.5.0",
    "tslib": "~2.3.0",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/localize": "~13.3.0",
    "@nguniversal/builders": "~13.1.0",
    "@types/express": "~4.17.0",
    "@types/jasmine": "~4.0.0",
    "@types/node": "~17.0.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-extract-i18n-merge": "^1.2.2",
    "npm-run-all": "~4.1.0",
    "typescript": "~4.6.0"
  }
}
